{
  "dhp290:0.1": "Khuddakanikāya ",
  "dhp290:0.2": "Dhammapada ",
  "dhp290:0.3": "Pakiṇṇakavagga ",
  "dhp290:0.4": "Attanopubbakammavatthu ",
  "dhp290:1": "Mattāsukhapariccāgā, ",
  "dhp290:2": "passe ce vipulaṁ sukhaṁ; ",
  "dhp290:3": "Caje mattāsukhaṁ dhīro, ",
  "dhp290:4": "sampassaṁ vipulaṁ sukhaṁ. ",
  "dhp291:0": "Kukkuṭaṇḍakhādikāvatthu ",
  "dhp291:1": "Paradukkhūpadhānena, ",
  "dhp291:2": "attano sukhamicchati; ",
  "dhp291:3": "Verasaṁsaggasaṁsaṭṭho, ",
  "dhp291:4": "verā so na parimuccati. ",
  "dhp292:0": "Bhaddiyānaṁbhikkhūnaṁvatthu ",
  "dhp292:1": "Yañhi kiccaṁ apaviddhaṁ, ",
  "dhp292:2": "akiccaṁ pana karīyati; ",
  "dhp292:3": "Unnaḷānaṁ pamattānaṁ, ",
  "dhp292:4": "tesaṁ vaḍḍhanti āsavā. ",
  "dhp293:1": "Yesañca susamāraddhā, ",
  "dhp293:2": "niccaṁ kāyagatā sati; ",
  "dhp293:3": "Akiccaṁ te na sevanti, ",
  "dhp293:4": "kicce sātaccakārino; ",
  "dhp293:5": "Satānaṁ sampajānānaṁ, ",
  "dhp293:6": "atthaṁ gacchanti āsavā. ",
  "dhp294:0": "Lakuṇḍakabhaddiyavatthu ",
  "dhp294:1": "Mātaraṁ pitaraṁ hantvā, ",
  "dhp294:2": "rājāno dve ca khattiye; ",
  "dhp294:3": "Raṭṭhaṁ sānucaraṁ hantvā, ",
  "dhp294:4": "anīgho yāti brāhmaṇo. ",
  "dhp295:1": "Mātaraṁ pitaraṁ hantvā, ",
  "dhp295:2": "rājāno dve ca sotthiye; ",
  "dhp295:3": "Veyagghapañcamaṁ hantvā, ",
  "dhp295:4": "anīgho yāti brāhmaṇo. ",
  "dhp296:0": "Dārusākaṭikaputtavatthu ",
  "dhp296:1": "Suppabuddhaṁ pabujjhanti, ",
  "dhp296:2": "sadā gotamasāvakā; ",
  "dhp296:3": "Yesaṁ divā ca ratto ca, ",
  "dhp296:4": "niccaṁ buddhagatā sati. ",
  "dhp297:1": "Suppabuddhaṁ pabujjhanti, ",
  "dhp297:2": "sadā gotamasāvakā; ",
  "dhp297:3": "Yesaṁ divā ca ratto ca, ",
  "dhp297:4": "niccaṁ dhammagatā sati. ",
  "dhp298:1": "Suppabuddhaṁ pabujjhanti, ",
  "dhp298:2": "sadā gotamasāvakā; ",
  "dhp298:3": "Yesaṁ divā ca ratto ca, ",
  "dhp298:4": "niccaṁ saṅghagatā sati. ",
  "dhp299:1": "Suppabuddhaṁ pabujjhanti, ",
  "dhp299:2": "sadā gotamasāvakā; ",
  "dhp299:3": "Yesaṁ divā ca ratto ca, ",
  "dhp299:4": "niccaṁ kāyagatā sati. ",
  "dhp300:1": "Suppabuddhaṁ pabujjhanti, ",
  "dhp300:2": "sadā gotamasāvakā; ",
  "dhp300:3": "Yesaṁ divā ca ratto ca, ",
  "dhp300:4": "ahiṁsāya rato mano. ",
  "dhp301:1": "Suppabuddhaṁ pabujjhanti, ",
  "dhp301:2": "sadā gotamasāvakā; ",
  "dhp301:3": "Yesaṁ divā ca ratto ca, ",
  "dhp301:4": "bhāvanāya rato mano. ",
  "dhp302:0": "Vajjiputtakabhikkhuvatthu ",
  "dhp302:1": "Duppabbajjaṁ durabhiramaṁ, ",
  "dhp302:2": "Durāvāsā gharā dukhā; ",
  "dhp302:3": "Dukkhosamānasaṁvāso, ",
  "dhp302:4": "Dukkhānupatitaddhagū; ",
  "dhp302:5": "Tasmā na caddhagū siyā, ",
  "dhp302:6": "Na ca dukkhānupatito siyā. ",
  "dhp303:0": "Cittagahapativatthu ",
  "dhp303:1": "Saddho sīlena sampanno, ",
  "dhp303:2": "yasobhogasamappito; ",
  "dhp303:3": "Yaṁ yaṁ padesaṁ bhajati, ",
  "dhp303:4": "tattha tattheva pūjito. ",
  "dhp304:0": "Cūḷasubhaddāvatthu ",
  "dhp304:1": "Dūre santo pakāsenti, ",
  "dhp304:2": "himavantova pabbato; ",
  "dhp304:3": "Asantettha na dissanti, ",
  "dhp304:4": "rattiṁ khittā yathā sarā. ",
  "dhp305:0": "Ekavihārittheravatthu ",
  "dhp305:1": "Ekāsanaṁ ekaseyyaṁ, ",
  "dhp305:2": "eko caramatandito; ",
  "dhp305:3": "Eko damayamattānaṁ, ",
  "dhp305:4": "vanante ramito siyā. ",
  "dhp305:5": "Pakiṇṇakavaggo ekavīsatimo. "
}